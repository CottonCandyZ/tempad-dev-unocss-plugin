{
  "name": "tempad-dev-plugin-unocss",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.14.0",
  "description": "UnoCSS plugin for TemPad Dev.",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "transform-to-unocss-core": "^0.0.69"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@antfu/ni": "^0.23.2",
    "@antfu/utils": "^0.7.10",
    "@figma/plugin-typings": "^1.107.0",
    "@s-libs/micro-dash": "^18.0.0",
    "@tempad-dev/plugins": "^0.4.10",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "lint-staged": "^15.4.3",
    "pnpm": "^9.15.5",
    "prettier": "^3.5.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "unbuild": "^2.0.0",
    "vite": "^6.1.0",
    "vue": "^3.5.13"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm build && git add dist"
  },
  "lint-staged": {
    "*": [
      "pnpm lint:fix",
      "pnpm format"
    ]
  }
}
